{
    "version": 1,
    "request": {
        "url": "https://api.spoonacular.com/recipes/complexSearch",
        "method": "GET",
        "authorization": {
            "type": "QueryParam",
            "keyParam": "apiKey"
        },
        "params": {
            "query": true,
            "minProtein": false,
            "minCarbs": false,
            "cuisine": false , 
            "diet": false, 
            "minProtein": false, 
            "maxCarbs": false, 
            "minCalories": false, 
            "maxCalories": false
        },
        "pagination": {
            "type": "offset",
            "offsetKey": "offset",
            "limitKey": "limit",
            "maxCount": 1000
        }
    },
    "response": {
        "ctype": "application/json",
        "tablePath": "$.results[*]",
        "schema": {
            "id": {
                "target": "$.id",
                "type": "int"
            },
            "calories": {
                "target": "$.calories",
                "type": "string"
            },
            "carbs": {
                "target": "$.carbs",
                "type": "string"
            },
            "fat": {
                "target": "$.fat",
                "type": "string"
            },
            "protein": {
                "target": "$.protein",
                "type": "string"
            },
            "title": {
                "target": "$.title",
                "type": "string"
            }
        },
        "orient": "records"
    }
}